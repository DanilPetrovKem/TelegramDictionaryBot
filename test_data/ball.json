            "lexemes": [
                {
                    "lemma": "ball",
                    "homographNumber": 1,
                    "partOfSpeech": "noun",
                    "senses": [
                        {
                            "definition": "A solid or hollow sphere, or roughly spherical mass.",
                            "labels": [
                                "countable",
                                "uncountable"
                            ],
                            "usageExamples": [
                                "a ball of spittle; a fecal ball"
                            ],
                            "subSenses": [
                                {
                                    "definition": "A quantity of string, thread, etc., wound into a spherical shape.",
                                    "usageExamples": [
                                        "a ball of wool; a ball of twine"
                                    ]
                                },
                                {
                                    "definition": "A solid, spherical nonexplosive missile for a cannon, rifle, gun, etc.",
                                    "context": {
                                        "domains": [
                                            "firearms",
                                            "mechanics",
                                            "physics",
                                            "weaponry"
                                        ]
                                    },
                                    "subSenses": [
                                        {
                                            "definition": "A jacketed non-expanding bullet, typically of military origin."
                                        },
                                        {
                                            "definition": "Such bullets collectively.",
                                            "labels": [
                                                "obsolete",
                                                "uncountable"
                                            ]
                                        }
                                    ]
                                },
                                {
                                    "definition": "A roundish protuberant portion of some part of the body.",
                                    "usageExamples": [
                                        "the ball of the thumb; the ball of the foot"
                                    ]
                                },
                                {
                                    "definition": "The front of the bottom of the foot, just behind the toes.",
                                    "context": {
                                        "domains": [
                                            "anatomy",
                                            "biology"
                                        ]
                                    }
                                },
                                {
                                    "definition": "The globe; the earthly sphere."
                                },
                                {
                                    "definition": "The set of points in a metric space lying within a given distance (the radius) of a given point; specifically, the homologue of the disk in a Euclidean space of any number of dimensions.",
                                    "context": {
                                        "domains": [
                                            "mathematics"
                                        ]
                                    }
                                },
                                {
                                    "definition": "(more generally) The set of points in a topological space lying within some open set containing a given point; the analogue of the disk in a Euclidean space.",
                                    "context": {
                                        "domains": [
                                            "mathematics"
                                        ]
                                    }
                                },
                                {
                                    "definition": "An object, generally spherical, used for playing games."
                                }
                            ]
                        },
                        {
                            "definition": "A round or ellipsoidal object.",
                            "labels": [
                                "countable",
                                "uncountable"
                            ],
                            "context": {
                                "domains": [
                                    "sports"
                                ]
                            },
                            "subSenses": [
                                {
                                    "definition": "Any sport or game involving a ball.",
                                    "usageExamples": [
                                        "George played his college ball at Stanford.",
                                        "The children were playing ball in the garden.",
                                        "The children were playing ball on the beach."
                                    ]
                                },
                                {
                                    "definition": "A pitch that falls outside of the strike zone.",
                                    "context": {
                                        "domains": [
                                            "baseball"
                                        ]
                                    }
                                },
                                {
                                    "definition": "An opportunity to launch the pinball into play.",
                                    "context": {
                                        "domains": [
                                            "pinball"
                                        ]
                                    },
                                    "usageExamples": [
                                        "If you get to a million points, you get another ball."
                                    ]
                                },
                                {
                                    "definition": "A single delivery by the bowler, six of which make up an over.",
                                    "context": {
                                        "domains": [
                                            "cricket"
                                        ]
                                    }
                                },
                                {
                                    "definition": "A pass; a kick of the football towards a teammate.",
                                    "context": {
                                        "domains": [
                                            "soccer"
                                        ]
                                    }
                                }
                            ]
                        },
                        {
                            "definition": "(mildly, usually in the plural) A testicle.",
                            "labels": [
                                "countable",
                                "slang",
                                "uncountable",
                                "vulgar"
                            ],
                            "subSenses": [
                                {
                                    "definition": "(in the plural) Nonsense.",
                                    "usageExamples": [
                                        "That’s a load of balls, and you know it!"
                                    ]
                                },
                                {
                                    "definition": "(in the plural) Courage.",
                                    "usageExamples": [
                                        "I doubt he’s got the balls to tell you off."
                                    ]
                                }
                            ]
                        },
                        {
                            "definition": "A leather-covered cushion, fastened to a handle called a ballstock; formerly used by printers for inking the form, then superseded by the roller.",
                            "labels": [
                                "countable",
                                "historical",
                                "uncountable"
                            ],
                            "context": {
                                "domains": [
                                    "printing"
                                ]
                            }
                        },
                        {
                            "definition": "A large pill, a form in which medicine was given to horses; a bolus.",
                            "labels": [
                                "countable",
                                "historical",
                                "uncountable"
                            ],
                            "context": {
